<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>New Coupon Form</title>
  </head>
  <body>
    <%= form_tag coupons_path do %>
      <label>Coupon Code:</label> <br>
      <%= text_field_tag :coupon_code %> <br>

      <label>Store:</label> <br>
      <%= text_field_tag :store %> <br>

      <input type="submit">
    <% end %>
  </body>
</html>
